{
  "name": "@bounceapp/iframe",
  "private": false,
  "license": "MIT",
  "description": "Iframe wrapper for React Native",
  "version": "0.4.0",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "react-native": "src/index",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index",
  "sideEffects": false,
  "files": [
    "src",
    "lib",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/*.stories.@(js|jsx|ts|tsx)"
  ],
  "keywords": [
    "expo",
    "iframe",
    "react-native",
    "iframe-react-native",
    "ios",
    "android",
    "web"
  ],
  "repository": "https://github.com/Bounceapp/iframe",
  "author": "Bounce (https://usebounce.com)",
  "bugs": {
    "url": "https://github.com/Bounceapp/iframe/issues"
  },
  "homepage": "https://github.com/Bounceapp/iframe#readme",
  "scripts": {
    "test": "jest --passWithNoTests",
    "typecheck": "tsc",
    "lint": "npx eslint . --ext .ts,.tsx,.js",
    "lint:fix": "npx eslint . --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "bob build && husky install",
    "release": "release-it",
    "build:docs": "storybook build --output-dir docs",
    "dev:docs": "storybook dev -p 6006",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npx prettier --write",
      "npx eslint --cache --fix"
    ],
    "*.{css,md,json}": "npx prettier --write"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@release-it/conventional-changelog": "5.1.1",
    "@storybook/addon-docs": "9.0.13",
    "@storybook/addon-links": "9.0.13",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/react-webpack5": "9.0.13",
    "@types/jest": "28.1.8",
    "@types/prettier": "2.7.3",
    "@types/react": "~19.0.10",
    "@types/react-dom": "~19.0.4",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-config-universe": "15.0.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-n": "14.0.0",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-native": "5.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "expo": "53.0.13",
    "husky": "8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "13.3.0",
    "prettier": "3.6.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-builder-bob": "0.40.12",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "release-it": "15.11.0",
    "storybook": "9.0.13",
    "typescript": "~5.8.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-web": "*",
    "react-native-webview": "*"
  }
}

{
  "name": "@bounceapp/iframe",
  "private": false,
  "license": "MIT",
  "description": "Iframe wrapper for React Native",
  "version": "0.1.3",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "react-native": "src/index",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index",
  "sideEffects": false,
  "files": [
    "src",
    "lib",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/*.stories.@(js|jsx|ts|tsx)"
  ],
  "keywords": [
    "expo",
    "iframe",
    "react-native",
    "iframe-react-native",
    "ios",
    "android",
    "web"
  ],
  "repository": "https://github.com/Bounceapp/iframe",
  "author": "Bounce (https://usebounce.com)",
  "bugs": {
    "url": "https://github.com/Bounceapp/iframe/issues"
  },
  "homepage": "https://github.com/Bounceapp/iframe#readme",
  "scripts": {
    "test": "jest --passWithNoTests",
    "typecheck": "tsc",
    "lint": "npx eslint . --ext .ts,.tsx,.js",
    "lint:fix": "npx eslint . --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "bob build && husky install",
    "release": "release-it",
    "build:docs": "build-storybook --output-dir docs",
    "dev:docs": "start-storybook -p 6006",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "npx eslint --cache --fix",
    "*.{ts,tsx,js,jsx,css,md,json}": "npx prettier --write"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@release-it/conventional-changelog": "5.1.1",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-interactions": "6.5.15",
    "@storybook/addon-links": "6.5.15",
    "@storybook/builder-webpack4": "6.5.15",
    "@storybook/manager-webpack4": "6.5.15",
    "@storybook/react": "6.5.15",
    "@storybook/testing-library": "0.0.13",
    "@types/jest": "28.1.8",
    "@types/prettier": "2.7.2",
    "@types/react": "~18.0.24",
    "@types/react-dom": "~18.0.8",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-universe": "11.1.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "expo": "^47.0.0",
    "husky": "8.0.2",
    "jest": "^26.6.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-builder-bob": "0.20.3",
    "react-native-web": "~0.18.7",
    "react-native-webview": "11.23.1",
    "release-it": "15.6.0",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-web": "*",
    "react-native-webview": "*"
  }
}
